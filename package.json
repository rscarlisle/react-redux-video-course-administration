{
  "name": "angular2-product-management-app",
  "version": "1.0.0",
  "description": "**Angular 2 - Product Management Application** ---------------",
  "main": "app.component.js",
  "scripts": {
    "prestart": "babel-node build/message.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test-watch start-mockapi",
    "open:src": "babel-node build/server.js",
    "lint": "esw webpack.config.* src build",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "test": "node_modules/mocha/bin/mocha",
    "test-all": "mocha --reporter progress build/testSetup.js \"src/**/*.test.js\"",
    "test-watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node build/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node build/build.js",
    "postbuild": "babel-node build/distServer.js",
    "deploy": "surge ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rscarlisle/angular2-product-management-app.git"
  },
  "keywords": [],
  "author": "Randall Carlisle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rscarlisle/angular2-product-management-app/issues"
  },
  "homepage": "https://github.com/rscarlisle/angular2-product-management-app#readme",
  "dependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/router": "^4.1.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "chalk": "^1.1.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "express-handlebars": "^3.0.0",
    "express-messages": "^1.0.1",
    "express-session": "^1.15.3",
    "express-validator": "^3.2.0",
    "moment": "^2.18.1",
    "mongodb": "^2.2.28",
    "mongoose": "^4.10.7",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "rxjs": "^5.4.0",
    "underscore": "^1.8.3",
    "whatwg-fetch": "^1.0.0",
    "zone.js": "^0.8.11"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^4.0.2",
    "compression": "^1.6.2",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-watch": "^3.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.8.0",
    "json-schema-faker": "^0.5.0-rc6",
    "json-server": "^0.10.1",
    "mocha": "^3.4.2",
    "nock": "^8.1.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "sinon": "^2.3.4",
    "style-loader": "^0.18.2",
    "surge": "^0.18.0",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5"
  }
}
